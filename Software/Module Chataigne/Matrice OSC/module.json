{
  "name": "Matrice OSC",
  "type": "OSC",
  "path": "Hardware",
  "version": "1.0.0",
  "description": "Controle Matrice X33D200",
  "url": "https://github.com/tommag/Sample-Chataigne-module",
  "downloadURL": "https://github.com/tommag/Sample-Chataigne-module/archive/master.zip",
  "hasInput": true,
  "hasOutput": true,
  "hideDefaultCommands": false,
  "defaults": {
    "autoAdd": false,
    "oscInput": {
      "localPort": 9001
    }
  },
  "parameters": {
    "Size X": {
      "type": "Integer",
      "default": 8
    },
    "Size Y": {
      "type": "Integer",
      "default": 8
    }
  },
  "scripts": [
    "matrice.js"
  ],
  "commands": {
    "Fixture": {
      "menu": "",
      "callback": "fixture",
      "parameters": {
        "X": {
          "type": "Integer",
          "default": 1
        },
        "Y": {
          "type": "Integer",
          "default": 1
        },
        "F": {
          "type": "Integer",
          "min": 1,
          "max": 8,
          "default": 1
        },
        "Couleur": {
          "type": "Color"
        }
      }
    },
    "Carré": {
      "menu": "",
      "callback": "square",
      "parameters": {
        "X": {
          "type": "Integer",
          "default": 1
        },
        "Y": {
          "type": "Integer",
          "default": 1
        },
        "Couleur": {
          "type": "Color"
        }
      }
    },
    "Matrice": {
      "menu": "",
      "callback": "matrix",
      "parameters": {
        "Couleur": {
          "type": "Color"
        }
      }
    },
    "Côté": {
      "menu": "",
      "callback": "side",
      "parameters": {
        "Côté": {
          "type": "Integer",
          "default": 1
        },
        "Couleur": {
          "type": "Color"
        }
      }
    },
    "Intersection": {
      "menu": "",
      "callback": "intersection",
      "parameters": {
        "X1": {
          "type": "Integer",
          "default": 1
        },
        "Y1": {
          "type": "Integer",
          "default": 1
        },
        "X2": {
          "type": "Integer",
          "default": 1
        },
        "Y2": {
          "type": "Integer",
          "default": 1
        },
        "Couleur": {
          "type": "Color"
        }
      }
    },
    "Spirale": {
      "menu": "",
      "callback": "spirale",
      "parameters": {
        "Taille": {
          "type": "Integer",
          "min": 1,
          "max": 4,
          "default": 4
        },
        "Couleur": {
          "type": "Color"
        }
      }
    },
    "Nettoyer": {
      "menu": "",
      "callback": "sendClear",
      "parameters": {
      }
    },
    "Intensité": {
      "menu": "",
      "callback": "sendIntensity",
      "parameters": {
        "Intensite": {
          "type": "Integer",
          "min": 0,
          "max": 100,
          "default": 100
        }
      }
    }
  }
}